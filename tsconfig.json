{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "dom"],
    "baseUrl": "./",
    "types": ["cypress", "node", "@badeball/cypress-cucumber-preprocessor"],
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule":true,
    "skipLibCheck": true
  },
  "include": ["**/*.ts"]
}
